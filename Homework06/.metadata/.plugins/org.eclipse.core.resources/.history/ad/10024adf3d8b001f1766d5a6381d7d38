#define SWITCHES_H_



#define DEBOUNCE_RESTART          (0x00) //
#define DEBOUNCE_TIME             (0x08) //
#define NOT_OKAY                  (0x00) //
#define OKAY                      (0x01) //
#define OFF                       (0x00) //
#define ON                        (0x01) //
#define RESTART                   (0x00) //
